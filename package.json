{
  "name": "itoc",
  "version": "2.2.5",
  "description": "itoc is a npm generator to help MarkDown file generate it's html with sidebar and can highlight the code.",
  "keywords": [
    "markdown",
    "md",
    "toc",
    "itoc"
  ],
  "main": "index.js",
  "scripts": {
    "start": "npm publish",
    "test": "node bin/cmd.js -f README.md -o -d"
  },
  "preferGlobal": "true",
  "bin": {
    "itoc": "bin/cmd.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itnik/itoc.git"
  },
  "author": "itnik",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/itnik/itoc/issues"
  },
  "dependencies": {
    "bufferhelper": "^0.2.1",
    "commander": "^2.9.0",
    "config": "^1.21.0",
    "handlebars": "^3.0.3",
    "marked": "^0.3.6",
    "object-assign": "^1.0.0",
    "open": "0.0.5",
    "shelljs": "^0.3.0",
    "archiver": "^1.2.0"
  },
  "devDependencies": {
    "gulp-livereload": "^2.1.1",
    "gulp-watch": "^0.7.0",
    "mocha": "*"
  },
  "homepage": "https://github.com/itnik/itoc",
  "directories": {
    "doc": "doc"
  }
}
